<html>
    <head>
        <meta charset="UTF-8"/>
        <title>Féelarsen - Dossier Général</title>

        <style>
            {% include 'back_office/artist/pdf/styles.css' %}
        </style>
    </head>
    <body>
        <!-- Couverture -->
        <section class="cover-page home">
            <svg class="cover-svg" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 210 260">
                <defs>
                    <clipPath id="clip-shape">
                        <path d="M 0 0 H 210 V 160 Q 105 260 0 160 Z"/>
                    </clipPath>
                </defs>
                <image href="file:///app/uploads/artist/feelarsen/pdf/cover.jpg"
                       width="210"
                       height="260"
                       clip-path="url(#clip-shape)"
                       preserveAspectRatio="xMidYMid slice"/>
            </svg>

            <div class="filetitle">
                <h1>Féelarsen</h1>
                <h2>Dossier Général</h2>
            </div>

            <div class="grid three">
                <div class="grid-item">
                    <a href="#">
                        <img src="file:///app/uploads/artist/feelarsen/pdf/qr_website.png" alt="feelarsen.fr"/>
                        Musique
                    </a>
                </div>

                <div class="grid-item">
                    <a href="https://feelarsen.fr">
                        <img src="file:///app/uploads/artist/feelarsen/pdf/qr_website.png" alt="feelarsen.fr"/>
                        Site web
                    </a>
                </div>

                <div class="grid-item">
                    <a href="#">
                        <img src="file:///app/uploads/artist/feelarsen/pdf/qr_website.png" alt="feelarsen.fr"/>
                        Clip vidéo
                    </a>
                </div>
            </div>
        </section>

        <!-- Saut de page -->
        <div class="page-break"></div>

        <!-- Présentation -->
        {% include 'back_office/artist/pdf/about/_cover.html.twig' %}
        {% include 'back_office/artist/pdf/about/_content.html.twig' %}

        <!-- Technique -->
        {% include 'back_office/artist/pdf/technical/_cover.html.twig' %}
        {% include 'back_office/artist/pdf/technical/_content.html.twig' %}

        <!-- Rider -->
        {% include 'back_office/artist/pdf/rider/_cover.html.twig' %}
        {% include 'back_office/artist/pdf/rider/_content.html.twig' %}

        <!-- Médias -->
        {# {% include 'back_office/artist/pdf/medias/_cover.html.twig' %}
        {% include 'back_office/artist/pdf/medias/_content.html.twig' %} #}

        <!-- Contacts -->
        {% include 'back_office/artist/pdf/contacts/_cover.html.twig' %}
        {% include 'back_office/artist/pdf/contacts/_content.html.twig' %}

        <!-- Saut de page -->
        <div class="page-break"></div>
        <div class="page-break"></div>

        <!-- Back -->
        {% include 'back_office/artist/pdf/_back.html.twig' %}
    </body>
</html>
